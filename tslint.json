{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "object-literal-sort-keys": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": [
      true,
      "log",
      "time",
      "timeEnd",
      "trace"
    ],
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      160
    ],
    "no-use-before-declare": true,
    "ordered-imports": [false],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      false
    ],
    "typedef": [
      true,
      "call-signature",
      "parameter"
    ]
  }
}
